  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Pytest",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["${relativeFileDirname}"],
      "justMyCode": true
    },
    {
      "name": "Python: Pytest with args",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["-m", "leetcode__best_time_to_buy_and_sell_stock", "-v", "-s"],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Pytest with args for app_config",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["--env=qa", "-v", "-s"],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
